{
  "name": "fflogs-analytics-infra",
  "private": true,
  "version": "0.1.0",
  "description": "AWS SAM IaC for FFLogs analytics backend",
  "scripts": {
    "clean": "rm -rf .aws-sam",
    "prepare:lambda": "rm -rf dist && mkdir -p dist && cp -R ../dist/* dist/",
    "preflight:xivapi": "node scripts/preflight-xivapi-icons.mjs",
    "sam:build": "sam build -t template.yaml",
    "sam:deploy:guided": "sam deploy --guided -t template.yaml",
    "sam:deploy": "sam deploy -t template.yaml"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.840.0",
    "@aws-sdk/lib-dynamodb": "^3.840.0"
  }
}
