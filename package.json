{
  "name": "logs-analytics",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint \"amplify/src/**/*.ts\" \"test/**/*.ts\" \"app/**/*.ts\" \"app/**/*.tsx\" \"lib/**/*.ts\"",
    "backend:build": "tsc -p amplify/tsconfig.build.json",
    "backend:dev": "tsx amplify/src/index.ts",
    "test": "vitest run",
    "amplify:prepare": "npm run backend:build",
    "infra:build": "npm --prefix infra run sam:build",
    "infra:deploy:guided": "npm --prefix infra run sam:deploy:guided",
    "infra:deploy": "npm --prefix infra run sam:deploy",
    "amplify:deploy": "npx ampx pipeline-deploy --branch ${AWS_BRANCH:-main} --app-id ${AWS_APP_ID}",
    "amplify:sandbox": "npx ampx sandbox"
  },
  "dependencies": {
    "dotenv": "^16.4.7",
    "next": "^15.5.12",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@aws-amplify/backend": "^1.21.0",
    "@aws-amplify/backend-cli": "^1.8.2",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "aws-cdk-lib": "^2.239.0",
    "constructs": "^10.5.1",
    "eslint": "^8.57.1",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3",
    "vitest": "^3.0.8"
  }
}
