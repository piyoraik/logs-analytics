{
  "name": "fflogs-poc",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "vitest run",
    "lint": "eslint \"src/**/*.ts\" \"test/**/*.ts\"",
    "web:clean": "rm -rf web/.next web/.next-web",
    "web:dev": "npm --prefix web run dev",
    "web:build": "npm --prefix web run build",
    "web:start": "npm --prefix web run start",
    "infra:prepare": "npm run build && npm --prefix infra run prepare:lambda",
    "infra:build": "npm --prefix infra run sam:build",
    "infra:deploy:guided": "npm --prefix infra run sam:deploy:guided",
    "infra:deploy": "npm --prefix infra run sam:deploy",
    "amplify:deploy": "npx ampx pipeline-deploy --branch ${AWS_BRANCH:-main} --app-id ${AWS_APP_ID}",
    "amplify:sandbox": "npx ampx sandbox"
  },
  "dependencies": {
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "@aws-amplify/backend": "^1.21.0",
    "@aws-amplify/backend-cli": "^1.8.2",
    "@types/node": "^22.13.10",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "aws-cdk-lib": "^2.239.0",
    "constructs": "^10.5.1",
    "eslint": "^8.57.1",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3",
    "vitest": "^3.0.8"
  }
}
